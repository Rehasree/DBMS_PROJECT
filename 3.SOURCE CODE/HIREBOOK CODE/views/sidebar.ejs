    /* Style the tab */
    .tab {
      float: left;

      background-color: #fff;
      width: 250px;
      height: 500px;
    }

    /* Style the buttons inside the tab */
    .tab button {
      display: block;
      background-color: inherit;
      color: black;
      padding: 22px 16px;
      width: 100%;
      border: none;
      outline: none;
      text-align: left;
      cursor: pointer;
      transition: 0.3s;
      font-size: 17px;
    }

    /* Change background color of buttons on hover */
    .tab button:hover {
      background-color: rgba(83, 46, 165, 0.1);
    }

    /* Create an active/current "tab button" class */
    .tab button.active {
      background-color: rgba(83, 46, 165, 0.1);
      border-right: 6px solid rgb(36, 22, 68);
    }

    /* Style the tab content */
    .tabcontent {

      float: left;
      padding: 0px 12px;

      width: 100%;
      border-left: none;

    }
  </style>

